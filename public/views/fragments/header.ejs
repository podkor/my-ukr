<header>
    <div id="homeHeader">
        <a href="/app" title="Home">
            <img style="border: 1px black;" height="75px"
                 src="https://upload.wikimedia.org/wikipedia/commons/7/79/%D0%A2%D1%80%D0%B8%D0%B7%D1%83%D0%B1.png"/>
        </a>
    </div>
    <div id="logoHeader">Ukr Dashboard
        <% if (username) { %>
            <div id="logInUser">
                <div>
                    <a id="username" href="/app/profile"><%- username %></a>
                </div>
                <div>
                    <a href="/app/auth/logout">Sign out</a>
                </div>
            </div>
        <% } else { %>
            <div id="logInUser">
                <div>
                    <a href="/app/auth/login">Sign in</a>
                </div>
            </div>
        <% } %>
    </div>
    <div style="height:5px; float: bottom; background-color: #0056B9"></div>
    <div style="height:5px; float: bottom; background-color: #FFD800"></div>
</header>
